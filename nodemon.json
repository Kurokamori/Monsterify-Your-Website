{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "public"
  ],
  "verbose": true,
  "execMap": {
    "js": "node"
  },
  "watch": [
    "src/discord/",
    "database/",
    "src/services/",
    "src/models/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json"
}
